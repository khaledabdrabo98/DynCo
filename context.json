{
    "@context": {
        "ex": "http://example.com/ns#",
        "s": "http://schema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",

        "repository_url": {
            "@reverse": "ex:issue",
            "@type": "@id"
        },
        "url": "@id",

        "number": {
            "@id": "s:identifier",
            "@type" : "xsd:integer"
        },
        "name": "s:name",
        "title": "s:name",
        "body": "s:description",
        "author_association": "ex:authorAssociation",
        "User": "ex:User",
        "user": "s:creator",
        "assignee": "ex:assignee",
        "login": {
            "@id": "s:accountId",
            "@type": "xsd:string"
        },
        "html_url": {
            "@id": "s:url",
            "@type" : "xsd:anyURI"
        },
        "labels": {
            "@reverse": "ex:label"
        },
        "issue_url": {
            "@reverse": "ex:comment",
            "@type": "@id"
        },
        "type": "@type",
        "state": "@type", 
        "open": "ex:OpenIssue",
        "closed": "ex:ClosedIssue",
        "closed_at": {
            "@id": "s:endDate",
            "@type": "xsd:dateTimeStamp"
        },
        "created_at": {
            "@id": "s:startDate",
            "@type": "xsd:dateTimeStamp"
        }
    }
}
 
 